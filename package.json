{
  "name": "@contentful/mcp-server",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/contentful-mcp-server"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "contentful-mcp-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "version": "node scripts/generate-version.js",
    "build": "tsc",
    "dev": "nodemon --ext ts --ignore build/ --watch src/ --exec 'npm run build && npm start'",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "npm run version && npm run build",
    "start": "node build/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "inspect": "node -r dotenv/config ./scripts/inspect.js",
    "inspect:watch": "node ./scripts/inspect-watch.js",
    "logs:watch": "npx mcps-logger",
    "update-licenses": "node scripts/update-licenses.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "contentful-management": "^11.52.2",
    "dotenv": "^16.4.7",
    "fast-xml-parser": "^5.2.0",
    "get-it": "^8.3.2",
    "gpt-tokenizer": "^2.9.0",
    "outdent": "^0.8.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "0.15.0",
    "@types/node": "^22.13.11",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "mcps-logger": "^1.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vitest": "^3.2.1"
  }
}
